#! /usr/bin/env bash

specs() {

[ "$1" != "--" ] && [ -z "$SPECS_INITIALIZED" ] && specs -- private initializeEnvironment init

  case "$1" in
    --)
      case "$2" in
        configuration)
            case "$3" in
              configurationFiles)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- configuration configurationFiles' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              configurationVariables)
                    case "$4" in
                      doSomething)
                        ## ### doSomething
            
                          ;;
                      *)
                        echo "Unknown 'specs -- configuration configurationVariables' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              environmentVariables)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- configuration environmentVariables' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- configuration' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        extensions)
            case "$3" in
              invocation)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- extensions invocation' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              registration)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- extensions registration' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- extensions' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        parallel)
            case "$3" in
              manage)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- parallel manage' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              queue)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- parallel queue' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              state)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- parallel state' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- parallel' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        reporters)
            case "$3" in
              finalResult)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- reporters finalResult' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              status)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- reporters status' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- reporters' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        runners)
            case "$3" in
              begin)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- runners begin' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              execute)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- runners execute' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- runners' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        specs)
            case "$3" in
              specFiles)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- specs specFiles' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              specFixtures)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- specs specFixtures' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              specFunctions)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- specs specFunctions' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              specHelperFiles)
                    case "$4" in
                      *)
                        echo "Unknown 'specs -- specs specHelperFiles' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- specs' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        x)
            case "$3" in
              dsls)
                    case "$4" in
                      bdd)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x dsls bdd' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      common)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x dsls common' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      xunit)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x dsls xunit' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      *)
                        echo "Unknown 'specs -- x dsls' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              formatters)
                    case "$4" in
                      specs)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x formatters specs' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      TAP)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x formatters TAP' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      xml)
                              case "$5" in
                                *)
                                  echo "Unknown 'specs -- x formatters xml' command: $5" >&2
                                  return 1
                                  ;;
                              esac
            
                          ;;
                      *)
                        echo "Unknown 'specs -- x formatters' command: $4" >&2
                        return 1
                        ;;
                    esac
      
                  ;;
              *)
                echo "Unknown 'specs -- x' command: $3" >&2
                return 1
                ;;
            esac
  
            ;;
        *)
          echo "Unknown 'specs --' command: $2" >&2
          return 1
          ;;
      esac

        ;;
    config)
      case "$2" in
        list)
          echo "Hi from config list"
  
            ;;
        *)
          echo "Unknown 'specs config' command: $2" >&2
          return 1
          ;;
      esac

        ;;
    --version)
      echo "specs v0.6.0"

        ;;
    *)
      echo "Unknown 'specs' command: $1" >&2
      return 1
      ;;
  esac

}

[ "${BASH_SOURCE[0]}" = "$0" ] && "specs" "$@"

